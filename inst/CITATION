citHeader("To cite magma in publications use:")
citEntry(entry="misc",
  title = "R package magma: Matrix Algebra on GPU and Multicore Architectures, Version 1.5.0",
  author = personList(person("Brian", "Smith", "J", "brian-j-smith@uiowa.edu"),
                      person("Serguei", "Sokol", "", "sokol@insa-toulouse.fr")),
  year = "October 19, 2014",
  note = "[On-line] http://cran.r-project.org/package=magma",
  textVersion = paste("Smith, B.J. and Sokol, S. (October 19, 2014)",
    "R package magma: Matrix Algebra on GPU and Multicore Architectures, Version 1.5.0,",
    "[On-line] http://cran.r-project.org/package=magma"),
)
